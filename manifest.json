{
  "manifest_version": 2,
  "name": "Prizedrop",
  "version": "0.1",
  "browser_action": {
    "default_popup": "dropdown.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ]
}
